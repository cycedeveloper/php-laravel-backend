{
    "name": "sayedsoft/dex-withdrawal",
    "description": "Dex withdrawal",
    "keywords": [
        "sayedsoft",
        "dex-withdrawal"
    ],
    "homepage": "https://github.com/sayedsoft/dex-withdrawal",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmad Yaman Sayed",
            "email": "yemen@sayedsoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sayedsoft\\DexWithdrawal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sayedsoft\\DexWithdrawal\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sayedsoft\\DexWithdrawal\\DexWithdrawalServiceProvider"
            ],
            "aliases": {
                "DexWithdrawal": "Sayedsoft\\DexWithdrawal\\DexWithdrawalFacade"
            }
        }
    }
}
