{
    "name": "sayedsoft/dex",
    "description": "Dex",
    "keywords": [
        "sayedsoft",
        "dex"
    ],
    "homepage": "https://github.com/sayedsoft/dex",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yaman Sayed",
            "email": "yemen@sayedsoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "iexbase/tron-api": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sayedsoft\\Dex\\": "src"
        },
        "files": [
            "src/Base/Helpers/NFfunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sayedsoft\\Dex\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sayedsoft\\Dex\\DexServiceProvider"
            ],
            "aliases": {
                "Dex": "Sayedsoft\\Dex\\DexFacade"
            }
        }
    }
}
