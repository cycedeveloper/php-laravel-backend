{
    "name": "sayedsoft/referral-tree",
    "description": "Referral income",
    "keywords": [
        "sayedsoft",
        "referral-tree"
    ],
    "homepage": "https://github.com/sayedsoft/referral-tree",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yaman",
            "email": "yemen@sayedsoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sayedsoft\\ReferralTree\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sayedsoft\\ReferralTree\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sayedsoft\\ReferralTree\\ReferralTreeServiceProvider"
            ],
            "aliases": {
                "ReferralTree": "Sayedsoft\\ReferralTree\\ReferralTreeFacade"
            }
        }
    }
}
