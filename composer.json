{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./packages/dex"
        },
        {
            "type": "path",
            "url": "./packages/dex-auth"
        },
        {
            "type": "path",
            "url": "./packages/referral-unilevel"
        },
        {
            "type": "path",
            "url": "./packages/withdrawal"
        },
        {
            "type": "path",
            "url": "./packages/referral-tree"
        },
        {
            "type": "path",
            "url": "./packages/exchange-token"
        },
        {
            "type": "path",
            "url": "./packages/stake"
        },
        {
            "type": "path",
            "url": "./packages/referral-income"
        },
        {
            "type": "path",
            "url": "./packages/withdrawal-fiat"
        }


       
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-bcmath": "*",
        "appoly/api-password-helper": "^2.3",
        "eminiarts/nova-tabs": "^2.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/horizon": "^5.10",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.3",
        "outl1ne/nova-settings": "^5.0",
        "predis/predis": "^2.1",
        "sayedsoft/dex": "*",
        "sayedsoft/dex-auth-referral": "*",
        "sayedsoft/dex-withdrawal": "*",
        "sayedsoft/exchange-token": "*",
        "sayedsoft/referral-income": "*",
        "sayedsoft/referral-tree": "*",
        "sayedsoft/referral-unilevel": "*",
        "sayedsoft/stake-token": "*",
        "sayedsoft/dex-withdrawal-fiat": "*",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "simplesquid/nova-advanced-number-field": "*",
        "spatie/laravel-responsecache": "^7.4",
        "spresnac/laravel-create-user-cli": "^1.0",
        "timothyasp/nova-badge-field": "^2.0",
        "titasgailius/search-relations": "^2.0",
        "wesselperik/nova-status-field": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "jeroen-g/laravel-packager": "^2.8",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan nova:publish"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
